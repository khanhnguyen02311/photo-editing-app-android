<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cardView_custom_item_gridview_account_page"
            app:cardCornerRadius="10dp"
            android:layout_margin="5dp"
            app:strokeWidth="2dp"
            app:strokeColor="#00FFFFFF">
        <!--app:strokeColor="#646464"
            app:strokeWidth="3dp"-->
                <com.example.photoeditingapp_main._Classes.SquareImageView
                    android:id="@+id/imageView_custom_item_gridview_account_page"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/welcome_background"
                    android:scaleType="centerCrop" />
                    <!--app:riv_corner_radius="12dp"
                    android:layout_margin="4dp"-->
        </com.google.android.material.card.MaterialCardView>

</RelativeLayout>